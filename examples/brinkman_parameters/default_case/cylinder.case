        "output_at_end": false,
        "output_boundary": false,
        "output_checkpoints": false,
        "end_time": 300,
        "timestep": 2.5e-3,
        "numerics": {
            "time_order": 3,
            "polynomial_order": 7,
            "dealias": true
        },
        "statistics": {
            "enabled": true,
            "output_control": "simulationtime",
            "output_value": 50.0,
            "start_time": 150.0,
