                    "type": "brinkman",
                    "objects": [
                        {
                            "type": "point_zone",
                            "name": "cylinder"
                        }
                    ],
                    "brinkman": {
                        "limits": [
                            0.0,
                            100.0
                        ],
                        "penalty": 1.0,
                        "implicit": true,
                    },
                    "filter": {
                        "type": "PDE",
                        "radius": 0.05
                    }
                }
            ]
        },
        "point_zones": [
            {
                "name": "cylinder",
